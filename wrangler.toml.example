name = "dreclaw"
main = "src/index.ts"
compatibility_date = "2026-02-22"
compatibility_flags = ["nodejs_compat"]
route = "REPLACE_WITH_WORKER_ROUTE"

[observability]
enabled = true

[[d1_databases]]
binding = "DRECLAW_DB"
database_name = "dreclaw"
database_id = "REPLACE_WITH_D1_DATABASE_ID"

[[r2_buckets]]
binding = "WORKSPACE_BUCKET"
bucket_name = "dreclaw-workspace"

[[durable_objects.bindings]]
name = "SESSION_RUNTIME"
class_name = "SessionRuntime"

[[migrations]]
tag = "v1"
new_sqlite_classes = ["SessionRuntime"]

[[migrations]]
tag = "v2"
new_sqlite_classes = ["Sandbox"]

[[migrations]]
tag = "v3"
deleted_classes = ["Sandbox"]
